<!DOCTYPE html>
<html>
<head>
	<title>TD Traveled Regions</title>
 <link rel="stylesheet" type="text/css" href="/DEALit/css/TD_List.css">

</head>

<body><center>
	<table border="5">
		<tr>
			<td><div id="of"><h1><center><a href="/td_DASHBOARD/TD_Traveled_Regions/insert">ADD LATEST Traveled Regions</a></center></h1></div></td>
			<td colspan="10"><h1>ALL RECORDED REGIONS THAT TRAVELED</h1>
				<div id="no"><u>TOTAL number of Traveled REGIONS: <%= td.length %></u></div>
      </td>
		</tr>

		<tr>
			<th>ID</th>
      <th>Traveler_NAME</th>
      <th>Region_NAME</th>
			<th>Used Vehicle</th>
			<th>Duration</th>
			<th>total Transportation Expense</th>
      <th>Date of Travel</th>
			<th colspan="8">ACTiONS</th>
		</tr>

		<% for(var i=0; i < td.length; i++){ %>
		<tr>
			<td><%= td[i].td_id %></td>
			<td><%= td[i].t_name %></td>
			<td><%= td[i].v_place %></td>
			<td><%= td[i].vehicle %></td>
			<td><%= td[i].duration %></td>
      <td><%= td[i].cost %></td>
      <td><%= td[i].date %></td>
			<td colspan="8">
				<div id="link">
				<a href="/td_DASHBOARD/TD_Traveled_Regions/edit/<%= td[i].td_id%>">UPDATE</a> ~
				<a href="/td_DASHBOARD/TD_Traveled_Regions/delete/<%= td[i].td_id%>">DELETE</a>
			</div>
			</td>
		</tr>
		<% } %>

		<tr>
				<td colspan="8"><div id="of"><a href="/td_DASHBOARD">H O M E</a></div></td>
		</tr>
			</table>

</body>
</html>
